{
  "ConnectionStrings": {
    "TradeDb": "Data Source=./data/TradeDb.dat"
  },
  "Messaging": {
    "Uri": "amqp://guest:guest@message-queue:5672",
    "ExchangeName": "StockExchange",
    "QueueName": "Trades",
    "RoutingKey": "trade-key"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "TradeBullProcessor"
    }
  }
}
