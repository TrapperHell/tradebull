{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TradeDb": "Data Source=./data/TradeDb.dat"
  },
  "Messaging": {
    "Uri": "amqp://guest:guest@message-queue:5672",
    "ExchangeName": "StockExchange",
    "QueueName": "Trades",
    "RoutingKey": "trade-key"
  },
  "Services": {
    "StockHistoryMaintenanceService": {
      "Enabled": true,
      "MarketName": "TSE",
      "MarketCloseDelay": "00:05:00"
    }
  }
}
